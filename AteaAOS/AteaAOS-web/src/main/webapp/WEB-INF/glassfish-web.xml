<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
    <context-root>/AteaAOS</context-root>
    <security-role-mapping>
        <role-name>superuser</role-name>
        <principal-name>superuser</principal-name>
        <group-name>superuser</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>admin</role-name>
        <principal-name>admin</principal-name>
        <group-name>admin</group-name>
    </security-role-mapping>
    <mail-resource description="AOS noreplyMail" from="aosnorepley@bamvpn.net" host="ut.mailadmin.no" jndi-name="mail/AOSMail" user="aosnorepley@bamvpn.net">
        <property name="mail-smtp-socketFactory-fallback" value="false"/>
        <property name="mail-smtp-starttls-enable" value="true"/>
        <property name="mail-smtp-socketFactory-class" value="javax.net.ssl.SSLSocketFactory"/>
        <property name="mail-smtp-socketFactory-port" value="465"/>
        <property name="mail-smtp-port" value="465"/>
        <property name="mail-smtp-auth" value="true"/>
        <property name="mail-smtp-password" value="aos-aos12345"/>
        <property name="mail-smtp-user" value="aosnorepley@bamvpn.net"/>
        <property name="mail-smtp-host" value="ut.mailadmin.no"/>
    </mail-resource>
    
    
    
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class' java code.</description>
        </property>
    </jsp-config>

</glassfish-web-app>
