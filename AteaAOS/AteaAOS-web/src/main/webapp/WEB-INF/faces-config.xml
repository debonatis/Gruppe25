<?xml version='1.0' encoding='UTF-8'?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <application>
        <navigation-rule>  
            <from-view-id>/index.xhtml</from-view-id>  
            <navigation-case>  
                <from-outcome>ok</from-outcome>  
                <to-view-id>/secure/GroupsUsers.xhtml</to-view-id>  
            </navigation-case>   
        </navigation-rule>  
        <navigation-rule>  
            <from-view-id>/secure/template/footer.xhtml</from-view-id>  
            <navigation-case>  
                <from-outcome>ok</from-outcome>  
                <to-view-id>/secure/uploadCSV.xhtml</to-view-id>  
            </navigation-case> 
            <navigation-case>  
                <from-outcome>users</from-outcome>  
                <to-view-id>/secure/UsersAdd.xhtml</to-view-id>  
            </navigation-case>   
        </navigation-rule> 
        <navigation-rule>  
            <from-view-id>/secure/adminPage.xhtml</from-view-id>  
            <navigation-case>  
                <from-outcome>View</from-outcome>  
                <to-view-id>/JSFtest/groups/View.xhtml</to-view-id>  
            </navigation-case>   
        </navigation-rule>    
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>        
    </application>
</faces-config>
